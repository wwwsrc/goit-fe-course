{{#each this}}

<li key="{{id}} id={{id}}" class="photo-card">
    <img src="{{webformatURL}}" alt="{{tags}}" data-largeImg="{{largeImageURL}}" width="300" />

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
        </p>
    </div>
</li>
{{/each}}
